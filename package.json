{
  "name": "px-converter",
  "version": "0.4.2",
  "author": "Alessio Michelini",
  "bin": {
    "px-converter": "./bin/run"
  },
  "bugs": "https://github.com/darkmavis1980/px-converter/issues",
  "dependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@typescript-eslint/parser": "^5.38.1",
    "babel-jest": "^29.3.1",
    "inquirer": "^8.2.4",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.3",
    "@types/inquirer": "^8.2.4",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.8.0",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "auto-changelog": "^2.4.0",
    "chai": "^4.3.6",
    "eslint": "^8.24.0",
    "eslint-config-oclif": "^4.0.0",
    "eslint-config-oclif-typescript": "^1.0.3",
    "eslint-plugin-unicorn": "^44.0.0",
    "globby": "^11.1.0",
    "nyc": "^15.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/darkmavis1980/px-converter",
  "keywords": [
    "converter"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "repository": "darkmavis1980/px-converter",
  "scripts": {
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "build": "tsc -b",
    "prepack": "rm -rf lib && npm run build",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "test:watch": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\" --watch",
    "changelog": "auto-changelog",
    "version": "auto-changelog -p && git add CHANGELOG.md",
    "eslint": "eslint . --ext .ts"
  },
  "types": "lib/index.d.ts",
  "auto-changelog": {
    "output": "CHANGELOG.md",
    "template": "changelog.tpl.hbs",
    "unreleased": true,
    "commitLimit": 10,
    "includeBranch": [
      "main",
      "dev"
    ]
  }
}
