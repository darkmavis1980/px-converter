{
  "name": "px-converter",
  "version": "0.7.2",
  "author": "Alessio Michelini",
  "bin": {
    "px-converter": "./bin/run"
  },
  "bugs": "https://github.com/darkmavis1980/px-converter/issues",
  "dependencies": {
    "commander": "^13.1.0",
    "inquirer": "^8.2.6",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@types/inquirer": "^8.2.10",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.27",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "auto-changelog": "^2.5.0",
    "babel-jest": "^29.7.0",
    "global": "^4.4.0",
    "globby": "^14.1.0",
    "jest": "^29.7.0",
    "nyc": "^17.1.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/darkmavis1980/px-converter",
  "keywords": [
    "converter"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "repository": "darkmavis1980/px-converter",
  "scripts": {
    "posttest": "npm run eslint",
    "build": "tsc -b",
    "prepack": "rm -rf lib && npm run build",
    "test": "jest --verbose",
    "test:watch": "jest --verbose --watch",
    "changelog": "auto-changelog",
    "version": "auto-changelog -p && git add CHANGELOG.md",
    "eslint": "npx eslint . --ext .ts",
    "prettier:check": "npx prettier ./src/**/*.ts ./test/**/*.test.ts --check",
    "prettier:write": "npx prettier ./src/**/*.ts ./test/**/*.test.ts --write"
  },
  "types": "lib/index.d.ts",
  "auto-changelog": {
    "output": "CHANGELOG.md",
    "template": "changelog.tpl.hbs",
    "unreleased": true,
    "commitLimit": 10,
    "includeBranch": [
      "main",
      "dev"
    ]
  }
}
