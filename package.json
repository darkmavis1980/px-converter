{
  "name": "px-converter",
  "version": "0.3.4",
  "author": "Alessio Michelini",
  "bin": {
    "px-converter": "./bin/run"
  },
  "bugs": "https://github.com/darkmavis1980/px-converter/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^5.1.10",
    "@typescript-eslint/parser": "^5.19.0",
    "cli-ux": "^6.0.9",
    "inquirer": "^8.2.2",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^2.0.3",
    "@types/chai": "^4.2.21",
    "@types/inquirer": "^8.2.1",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "auto-changelog": "^2.3.0",
    "chai": "^4.3.6",
    "eslint": "^8.13.0",
    "eslint-config-oclif": "^4.0.0",
    "eslint-config-oclif-typescript": "^1.0.2",
    "eslint-plugin-unicorn": "^42.0.0",
    "globby": "^11.0.4",
    "mocha": "^9.2.2",
    "nyc": "^15.1.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/darkmavis1980/px-converter",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "px-converter",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "darkmavis1980/px-converter",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "changelog": "auto-changelog",
    "version": "oclif-dev readme && git add README.md",
    "eslint": "eslint . --ext .ts"
  },
  "types": "lib/index.d.ts",
  "auto-changelog": {
    "output": "CHANGELOG.md",
    "template": "changelog.tpl.hbs",
    "unreleased": true,
    "commitLimit": 10,
    "includeBranch": [
      "master",
      "dev"
    ]
  }
}
