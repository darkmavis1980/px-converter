{
  "name": "px-converter",
  "version": "0.2.23",
  "author": "Alessio Michelini",
  "bin": {
    "px-converter": "./bin/run"
  },
  "bugs": "https://github.com/darkmavis1980/px-converter/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.1",
    "cli-ux": "^5.5.1",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.22",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "auto-changelog": "^2.2.1",
    "chai": "^4.2.0",
    "eslint": "^7.18.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.2.0",
    "globby": "^11.0.2",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/darkmavis1980/px-converter",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "px-converter",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "darkmavis1980/px-converter",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "changelog": "auto-changelog",
    "version": "oclif-dev readme && git add README.md",
    "eslint": "eslint . --ext .ts"
  },
  "types": "lib/index.d.ts",
  "auto-changelog": {
    "output": "CHANGELOG.md",
    "template": "changelog.tpl.hbs",
    "unreleased": true,
    "commitLimit": 10,
    "includeBranch": [
      "master",
      "dev"
    ]
  }
}
